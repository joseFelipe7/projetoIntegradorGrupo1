<% if(data.cliente.clientes_enderecos != ""){ %> 
 <div class="card-body">
          <!-- formulario endereço -->
     <% data.cliente.clientes_enderecos.forEach(cliente_endereco => { %>

          <form action="/usuario/area-contratante/meus-dados/<%=cliente_endereco.id%>/endereco" method="GET">
            <!-- input linha 1 -->
            <div class="form-row">
              <!-- input data cep -->
              <div class="form-group col">
                <label for="cep">CEP</label>
                <div class="input-group">
                  <div class="input-group-prepend">
                    <span class="input-group-text etiquetaCampo">CEP</span>
                  </div>
                  <input type="number" name="contratanteCep" class="form-control campoMeusDadosStyle campoDadoReadonly" id="cep" value="<%= cliente_endereco.cep %>" readonly maxlength="8" pattern="[0-9]+$">
                </div>
              </div>
            </div>
          <!-- input linha 2 -->
            <div class="form-row">
              <!-- input UF -->
              <div class="form-group col-md-6">
                <label for="UF">UF</label>
                <div class="input-group">
                  <div class="input-group-prepend">
                    <span class="input-group-text etiquetaCampo">UF</span>
                  </div>
                  <input type="text" name="contratanteUf" class="form-control campoMeusDadosStyle campoDadoReadonly" id="UF" value="<%= cliente_endereco.uf %>" readonly>
                </div>
              </div>

              <!-- input cidade -->
              <div class="form-group col-md-6">
                <label for="cidade">Cidade</label>
                <div class="input-group">
                  <div class="input-group-prepend">
                    <span class="input-group-text etiquetaCampo">Cidade</span>
                  </div>
                  <input type="text" name="contratanteCidade" class="form-control campoMeusDadosStyle campoDadoReadonly" id="cidade" value="<%= cliente_endereco.cidade %>" readonly>
                </div>
              </div>
            </div>

            <!-- input linha 3 -->
            <div class="form-row">
              <!-- input rua -->
              <div class="form-group col-md-5">
                <label for="rua">Rua</label>
                <div class="input-group">
                  <div class="input-group-prepend">
                    <span class="input-group-text etiquetaCampo">Rua</span>
                  </div>
                  <input type="text" name="contratanteRua" class="form-control campoMeusDadosStyle campoDadoReadonly" id="rua" value="<%= cliente_endereco.logradouro %>" readonly>
                </div>
              </div>

              <!-- input numero -->
              <div class="form-group col-md-2">
                <label for="numero">Nº</label>
                <div class="input-group">
                  <div class="input-group-prepend">
                    <span class="input-group-text etiquetaCampo">Nº</span>
                  </div>
                  <input type="number" name="contratanteN" class="form-control campoMeusDadosStyle campoDadoReadonly" id="numero" value="<%= cliente_endereco.numero %>" readonly pattern="[0-9]+$">
                </div>
              </div>

              <!-- input complemento -->
              <div class="form-group col-md-5">
                <label for="complemento">Complemento</label>
                <div class="input-group">
                  <div class="input-group-prepend">
                    <span class="input-group-text etiquetaCampo">Complemento</span>
                  </div>
                  <input type="text" name="contratanteComplemento" class="form-control campoMeusDadosStyle campoDadoReadonly" id="complemento" value="<%= cliente_endereco.complemento %>" readonly>
                </div>
              </div>
            </div>

            <!-- botao editar ABRE O MODAL-->
            <!-- Button trigger modal -->
            <div class="d-flex justify-content-end"><button type="button" class="btn botaoMeusDados" data-toggle="modal" data-target="#editarDadosEnderecoContratante">Editar</button></div>
          </form>
        <% }) %> 

        </div>
     
<!-- Modal ------------------------------------------------->


<!-- fim Modal ------------------------------------------------->
<% } else { %>
 
  <div class="card-body">
        <!-- formulario endereço -->
        <form> 
          <!-- input linha 1 -->
