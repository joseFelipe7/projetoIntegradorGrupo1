<% if(data.prestador.prestadores_enderecos != "") {%>

        <div class="card-body">
          <!-- formulario endereço -->
  
  <!--FORM GET - SHOW-->
          <form action="/usuario/area-prestador/meusDados/<%=data.prestador.id%>/endereco" method="GET">
            <!-- input linha 1 -->
            <div class="form-row">
              <!-- input data cep -->
              <div class="form-group col">
                <label for="cep">CEP</label>
                <div class="input-group">
                  <div class="input-group-prepend">
                    <span class="input-group-text etiquetaCampo">CEP</span>
                  </div>
                  <input type="number" class="form-control campoMeusDadosStyle campoDadoReadonly" id="cep"
                    value="<%= data.prestador.cep %>" readonly maxlength="8" pattern="[0-9]+$">
                </div>
              </div>
            </div>
            <!-- input linha 2 -->
            <div class="form-row">
              <!-- input UF -->
              <div class="form-group col-md-6">
                <label for="UF">UF</label>
                <div class="input-group">
                  <div class="input-group-prepend">
                    <span class="input-group-text etiquetaCampo">UF</span>
                  </div>
                  <input type="text" class="form-control campoMeusDadosStyle campoDadoReadonly" id="UF"
                    value="<%= data.prestador.uf %>" readonly>
                </div>
              </div>
  
              <!-- input cidade -->
              <div class="form-group col-md-6">
                <label for="cidade">Cidade</label>
                <div class="input-group">
                  <div class="input-group-prepend">
                    <span class="input-group-text etiquetaCampo">Cidade</span>
                  </div>
                  <input type="text" class="form-control campoMeusDadosStyle campoDadoReadonly" id="cidade"
                    value="<%= data.prestador.cidade %>" readonly>
                </div>
              </div>
            </div>
  
            <!-- input linha 3 -->
            <div class="form-row">
              <!-- input rua -->
              <div class="form-group col-md-5">
                <label for="rua">Rua</label>
                <div class="input-group">
                  <div class="input-group-prepend">
                    <span class="input-group-text etiquetaCampo">Rua</span>
                  </div>
                  <input type="text" class="form-control campoMeusDadosStyle campoDadoReadonly" id="rua"
                    value="<%= data.prestador.logradouro %>" readonly>
                </div>
              </div>
  
              <!-- input numero -->
              <div class="form-group col-md-2">
                <label for="numero">Nº</label>
                <div class="input-group">
                  <div class="input-group-prepend">
                    <span class="input-group-text etiquetaCampo">Nº</span>
                  </div>
                  <input type="number" class="form-control campoMeusDadosStyle campoDadoReadonly" id="numero"
                    value="<%= data.prestador.numero %>" readonly pattern="[0-9]+$">
                </div>
              </div>
  
              <!-- input complemento -->
              <div class="form-group col-md-5">
                <label for="complemento">Complemento</label>
                <div class="input-group">
                  <div class="input-group-prepend">
                    <span class="input-group-text etiquetaCampo">Complemento</span>
                  </div>
                  <input type="text" class="form-control campoMeusDadosStyle campoDadoReadonly" id="complemento"
                    value="<%= data.prestador.complemento %>" readonly>
                </div>
              </div>
            </div>
  
            <!-- botao editar ABRE O MODAL-->
            <!-- Button trigger modal -->
            <div class="d-flex justify-content-end">
              <button type="button" class="btn botaoMeusDados">Editar</button>
            </div>
          </form>
  
        </div>
  
    <!-- Modal ------------------------------------------------->
  
   
    <% } else { %>
  
