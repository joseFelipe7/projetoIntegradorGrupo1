<!-- Modal -->

      <div class="modal-body bg-roxoClaro">

        <!-- formulario conta -->
        <form action="/usuario/area-prestador/meusDados/<%=data.prestador.id%>" method="GET">
          <!-- input linha 1 -->
          <div class="form-row">
            <!-- input senha -->
            <div class="form-group col">
              <label for="senha">Senha Atual</label>
              <div class="input-group">
                <div class="input-group-prepend">
                  <span class="input-group-text etiquetaCampo">Senha</span>
                </div>
                <input type="password" class="form-control campoMeusDadosStyle campoDadoReadonly"
                  value="<%= data.prestador.senha %>" id="senha1p" placeholder="Digite sua senha atual" minlength="8">
              </div>
            </div>
          </div>

          <div class="form-row">
            <!-- input senha -->
            <div class="form-group col">
              <label for="senha">Nova senha</label>
              <div class="input-group">
                <div class="input-group-prepend">
                  <span class="input-group-text etiquetaCampo">Senha</span>
                </div>
                <input type="password" class="form-control campoMeusDadosStyle campoDadoReadonly" id="senha2p"
                  placeholder="Digite sua nova senha" minlength="8">
              </div>
            </div>
          </div>

          <!-- input linha 2 -->
          <div class="form-row">
            <!-- input email -->
            <div class="form-group col">
              <label for="email">E-mail de cadastro</label>
              <div class="input-group">
                <div class="input-group-prepend">
                  <span class="input-group-text etiquetaCampo">@</span>
                </div>
                <input type="email" class="form-control campoMeusDadosStyle campoDadoReadonly" id="emailp"
                  value="<%= data.prestador.email_principal %>" placeholder="Digite seu e-mail">
              </div>
            </div>
          </div>

          <!-- botao editar ABRE O MODAL-->
          <!-- Button trigger modal -->
          <div class="d-flex justify-content-end"><button type="button" class="btn botaoMeusDados" data-toggle="modal"
              data-target="#editarDadosContaContratante">SALVAR</button></div>
        </form>

      </div>
   
<!-- fim Modal ------------------------------------------------->