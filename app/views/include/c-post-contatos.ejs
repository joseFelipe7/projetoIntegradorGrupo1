<div class="modal-body bg-roxoClaro">
        
        <!-- formulario contato -->
        <% data.cliente.contatos.forEach(contato => { %>

          <form action="/usuario/area-contratante/meus-dados/contatos/<% data.cliente.id %> ?_method=PUT" id="formc" method="POST"> 
            <!-- input linha 1 -->
              <div class="form-row">
                <!-- input cel1 -->
                <div class="form-group col-md-6">
                  <label for="cel1">Celular 1</label>
                  <div class="input-group">
                    <div class="input-group-prepend">
                      <span class="input-group-text etiquetaCampo">Celular 1</span>
                    </div>
                    <input type="tel" name="contratanteCelular1" class="form-control campoMeusDadosStyle campoDadoReadonly" id="cel1" placeholder="Digite seu celular - apenas números" maxlength="11" pattern="\([0-9]{2}\)[0-9]{4,6}-[0-9]{3,4}$" value="<%= contato.celular_principal %>">
                  </div>
                </div>

                <!-- input cel2 -->
                <div class="form-group col-md-6">
                  <label for="cel2">Celular 2</label>
                  <div class="input-group">
                    <div class="input-group-prepend">
                      <span class="input-group-text etiquetaCampo">Celular 2</span>
                    </div>
                    <input type="tel" name="contratanteCelular2" class="form-control campoMeusDadosStyle campoDadoReadonly" id="cel2" placeholder="Digite seu celular - apenas números" maxlength="11" pattern="\([0-9]{2}\)[0-9]{4,6}-[0-9]{3,4}$" value="<%= contato.celular_secundario %>">
                  </div>
                </div>
              </div>

              <!-- input linha 2 -->
              <div class="form-row">
                <!-- input telefone -->
                <div class="form-group col">
                  <label for="tel">Telefone</label>
                  <div class="input-group">
                    <div class="input-group-prepend">
                      <span class="input-group-text etiquetaCampo">Telefone</span>
                    </div>
                    <input type="tel" name="contratanteTelefone" class="form-control campoMeusDadosStyle campoDadoReadonly" id="tel" placeholder="Digite seu telefone - apenas números" maxlength="10" pattern="\([0-9]{2}\)[0-9]{4,6}-[0-9]{3,4}$" value="<%= contato.telefone_residencial %>">
                  </div>
                </div>
              </div>

              <!-- input linha 3 -->
              <div class="form-row">
                <!-- input email -->
                <div class="form-group col">
                  <label for="email">E-mail principal</label>
                  <div class="input-group">
                    <div class="input-group-prepend">
                      <span class="input-group-text etiquetaCampo">@</span>
                    </div>
                    <input type="email" name="contratanteEmail" class="form-control campoMeusDadosStyle campoDadoReadonly" id="email1" value="<%= contato.email_principal %>">
                  </div>
                </div>
              </div>
              <!-- input linha 3 -->
              <div class="form-row">
                <!-- input email -->
                <div class="form-group col">
                  <label for="email">E-mail secundário</label>
                  <div class="input-group">
                    <div class="input-group-prepend">
                      <span class="input-group-text etiquetaCampo">@</span>
                    </div>
                    <input type="email" name="contratanteEmail2" class="form-control campoMeusDadosStyle campoDadoReadonly" id="email2" placeholder="Digite seu e-mail" value="<%= contato.email_secundario %>">
                  </div>
                </div>
              </div>
              <div id="error-list" class="alert alert-danger alert-dismissible fade show" role="alert" hidden>
                <ul>
                </ul>
                <button type="button" class="close" id="close-listP" data-dismiss="alert" aria-label="Close">
                  <span aria-hidden="true">&times;</span>
                </button>
           
              </div>

              <!-- botao editar ABRE O MODAL-->
              <!-- Button trigger modal -->
              <div class="d-flex justify-content-end">
                <button type="submit" class="btn botaoMeusDados" >SALVAR</button>
              </div>
              
            </form>
        <% }) %> 
        
       
      </div>


