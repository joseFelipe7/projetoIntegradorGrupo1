  <div id="accordion">
    <% if(!data.cliente.contatos || !data.cliente.endereco) { %>
      <p class="finalizarCad"><img src="/img/alert.svg" alt="">Finalize seu cadastro</p>
    <% } %> 

    <!-- card sanfona 1 ------------------------------------------------->
    <div class="card">
      <!-- topo 1 sanfona -->
      <div class="card-header sanfonaTopoMeusDados" id="headingOne">
        <h5 class="mb-0">
          
          <button class="btn btn-link tituloSanfonaTopoMeusDados" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
            <img src="/img/dados.png" alt="Meus dados" title="Meus dados">
            Pessoais
          </button>
        </h5>
      </div>
  
      <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordion">
        <div class="card-body">
          <!-- formulario dados pessoais -->
          
         
            <form action="/usuario/area-contratante/meus-dados/<%=data.cliente.id%>" method="GET"> 
              <!-- linha foto -->
              <div class="form-row">
                <!-- input data nascimento -->
                <div class="form-group col">
                  <label for="nascimento">Perfil </label>
                  <div class="input-group">
                    <% if(data.cliente.avatar == 0){ %>
                      <img src="/img/userEmpty.jpeg" width="120px" height="120px" alt="Meu Perfil" title="Meu Perfil" class="img-thumbnail perfilMeusDados">
                    <% } else {%> 
                      <img src="<%= data.cliente.avatar %>" width="120px" height="120px" alt="Meu Perfil" title="Meu Perfil" class="img-thumbnail perfilMeusDados">

                    <% } %> 
                  </div>
                </div>
              </div>
  
            <!-- input linha 1 -->
              <div class="form-row">
                <!-- input nome -->
                <div class="form-group col-md-6">
                  <label for="nome">Nome </label>
                  <div class="input-group">
                    <div class="input-group-prepend">
                      <span class="input-group-text etiquetaCampo">Nome</span>
                    </div>
                    <input type="text" name="contratanteNome" class="form-control campoMeusDadosStyle campoDadoReadonly" id="nomec" value="<%= data.cliente.nome %>" readonly>
                  </div>
                </div>
  
                <!-- input cpf -->
                <div class="form-group col-md-6">
                  <label for="cpf">CPF</label>
                  <div class="input-group">
                    <div class="input-group-prepend">
                      <span class="input-group-text etiquetaCampo">CPF</span>
                    </div>
                    <input type="number" name="contratanteCpf" class="form-control campoMeusDadosStyle campoDadoReadonly" id="cpfc" value="<%= data.cliente.cpf %>" readonly maxlength="11" pattern="[0-9]+$">
                  </div>
                </div>
              </div>
  
              <!-- input linha 2 -->
              <div class="form-row">
                <!-- input data nascimento -->
                <div class="form-group col">
                  <label for="nascimento">Data Nascimento </label>
                  <div class="input-group">
                    <div class="input-group-prepend">
                      <span class="input-group-text etiquetaCampo">Data Nascimento</span>
                    </div>
                    <input type="date" name="contratanteNascimento" class="form-control campoMeusDadosStyle campoDadoReadonly" id="nascimentoc" value="<%= data.moment(data.cliente.data_nascimento).format('YYYY-MM-DD') %>" readonly>
                  </div>
                </div>
              </div>

  
              <!-- botao editar ABRE O MODAL-->
              <!-- Button trigger modal -->
              <div class="d-flex justify-content-end">
                <button type="button" class="btn botaoMeusDados" data-toggle="modal" data-target="#editarDadosPessoaisContratante">Editar</button>
              </div>
            </form>
         
        </div>
      </div>
    </div>
