            </div>
        
            <div class="form-group col-9">
              <label for="photo">Envie sua foto</label>
              <div class="input-group d-flex justify-content-center">
                  <div class="custom-file align-self-center">
                    <input type="file" name="photo" id="photo" class="custom-file-input" value="<%= data.cliente.avatar %>">
                    <label class="custom-file-label" name="photo" for="photo">Escolha um arquivo</label>
                  </div>
              </div>
            </div>
            
            
          </div>
        <!-- input linha 1 -->
          <div class="form-row">
            <!-- input nome -->
            <div class="form-group col-md-6">
              <label for="nome">Nome <span class="campoObrigatorio">*</span></label>
              <div class="input-group">
                <div class="input-group-prepend">
                  <span class="input-group-text etiquetaCampo">Nome</span>
                </div>
                <input type="text" name="contratanteNome" class="form-control campoMeusDadosStyle campoDadoReadonly" id="nomecUp" value="<%= data.cliente.nome %>">
              </div>
            </div>
            <!-- input cpf -->
            <div class="form-group col-md-6">
              <label for="cpf">CPF</label>
              <div class="input-group">
                <div class="input-group-prepend">
                  <span class="input-group-text etiquetaCampo">CPF</span>
                </div>
                <input type="number" name="contratanteCpf" class="form-control campoMeusDadosStyle campoDadoReadonly" id="cpfcUp" value="<%= data.cliente.cpf %>" maxlength="11" pattern="[0-9]+$" disabled>
              </div>
            </div>
          </div>
          <!-- input linha 2 -->
          <div class="form-row">
            <!-- input data nascimento -->
            <div class="form-group col">
              <label for="nascimento">Data Nascimento <span class="campoObrigatorio">*</span></label>
              <div class="input-group">
                <div class="input-group-prepend">
                  <span class="input-group-text etiquetaCampo">Data Nascimento</span>
                </div>
                <input type="date" name="contratanteNascimento" class="form-control campoMeusDadosStyle campoDadoReadonly" id="nascimentocUp" value="<%= data.cliente.data_nascimento %>">
              </div>
            </div>
          </div>
          <label for="">(<span class="campoObrigatorio">*</span>) Campos obrigatórios</label>

          <!--div validação-->
          <div id="error-list" class="alert alert-danger alert-dismissible fade show" role="alert" hidden>
            <ul>
            </ul>
          <button type="button" class="close" id="close-listP" data-dismiss="alert" aria-label="Close">
          <span aria-hidden="true">&times;</span>
            </button>
       
          </div>
               <!-- botao editar ABRE O MODAL-->
            <!-- Button trigger modal -->
            <div class="d-flex justify-content-end">
                <button type="submit" class="btn botaoMeusDados" id="botaodp">SALVAR</button>
            </div>
         
        </form>
       
